{
  "name": "react-generator-component",
  "displayName": "react-generator-component",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.44.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:react-generator-component.createComponent",
    "onCommand:react-generator-component.createComponentWithState",
    "onCommand:react-generator-component.createComponentWithStyle",
    "onCommand:react-generator-component.createComponentWithStateAndStyle"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "react-generator-component.createComponent",
        "title": "Create component"
      },
      {
        "command": "react-generator-component.createComponentWithState",
        "title": "Create component with state"
      },
      {
        "command": "react-generator-component.createComponentWithStyle",
        "title": "Create component with style"
      },
      {
        "command": "react-generator-component.createComponentWithStateAndStyle",
        "title": "Create component with state and style"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "react-generator-component.createComponent",
          "group": "createFileCommands@1",
          "when": "explorerResourceIsFolder"
        },
        {
          "command": "react-generator-component.createComponentWithState",
          "group": "createFileCommands@1",
          "when": "explorerResourceIsFolder"
        },
        {
          "command": "react-generator-component.createComponentWithStyle",
          "group": "createFileCommands@1",
          "when": "explorerResourceIsFolder"
        },
        {
          "command": "react-generator-component.createComponentWithStateAndStyle",
          "group": "createFileCommands@1",
          "when": "explorerResourceIsFolder"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/change-case": "^2.3.1",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.44.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "change-case": "^4.1.1"
  }
}
